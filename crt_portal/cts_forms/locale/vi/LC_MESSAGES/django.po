#: question_text.py:8
msgid "First name"
msgstr "Tên"

#: question_text.py:9
msgid "Last name"
msgstr "Họ"

#: question_text.py:10
msgid "Email address"
msgstr "Địa chỉ email"

#: question_text.py:11
msgid "Phone number"
msgstr "Số điện thoại"

#: question_text.py:12
msgid "Mailing address 1"
msgstr "Địa chỉ nhận thư 1"

#: question_text.py:13
msgid "Mailing address 2"
